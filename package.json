{
    "name": "wooden-stone-website",
    "version": "1.0.0",
    "description": "The Wooden Stone LLC Official Company Website",
    "main": "backend/server.js",
    "scripts": {
        "start": "node backend/server.js",
        "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
        "dev:frontend": "python -m http.server 8000 --directory src/pages",
        "dev:backend": "nodemon backend/server.js",
        "build": "node build.js"
    },
    "keywords": [
        "construction",
        "countertops",
        "cabinets",
        "commercial",
        "multifamily"
    ],
    "author": "The Wooden Stone LLC",
    "license": "MIT",
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "express-validator": "^7.0.1",
        "fs-extra": "^11.3.1",
        "helmet": "^7.1.0",
        "multer": "^2.0.2",
        "postmark": "^4.0.0"
    },
    "devDependencies": {
        "concurrently": "^8.2.0",
        "nodemon": "^3.0.2"
    },
    "engines": {
        "node": ">=14.0.0"
    }
}